{
  "name": "heroin",
  "description": "Dependency injection done right",
  "version": "0.6.0",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "bugs": {
    "url": "https://github.com/bahmutov/heroin/issues"
  },
  "contributors": [],
  "dependencies": {},
  "devDependencies": {
    "expect.js": "0.3.1",
    "grunt": "0.4.5",
    "grunt-clean-console": "0.3.0",
    "grunt-cli": "0.1.13",
    "grunt-contrib-jshint": "0.11.3",
    "grunt-contrib-watch": "0.6.1",
    "grunt-deps-ok": "0.8.0",
    "grunt-mocha-test": "0.12.7",
    "grunt-nice-package": "0.9.4",
    "jshint-stylish": "2.0.1",
    "lodash": "3.10.1",
    "matchdep": "1.0.0",
    "pkgfiles": "^2.3.0",
    "pre-git": "^1.3.1",
    "time-grunt": "1.2.2"
  },
  "engines": {
    "node": "> 0.8"
  },
  "homepage": "https://github.com/bahmutov/heroin",
  "keywords": [
    "dependency",
    "di",
    "injection",
    "javascript"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:bahmutov/heroin.git"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "test": "grunt",
    "commit": "commit-wizard",
    "pkgfiles": "pkgfiles",
    "size": "tarball=\"$(npm pack .)\"; wc -c \"${tarball}\"; tar tvf \"${tarball}\"; rm \"${tarball}\";"
  },
  "config": {
    "pre-git": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": [
        "npm test"
      ],
      "pre-push": [
        "npm run size",
        "npm run pkgfiles"
      ],
      "post-commit": [],
      "post-merge": []
    }
  }
}
